{
    "name": "nutribyte",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "dev": "vite --config vite.config.js --host --port 8080",
        "build": "vite build --config vite.config.js",
        "lint": "eslint --config eslint.config.js src --ext js,jsx --report-unused-disable-directives --ignore-pattern '**/tests/**'",
        "lint:strict": "eslint --config eslint.config.js src --ext js,jsx --report-unused-disable-directives --max-warnings 0 --ignore-pattern '**/tests/**'",
        "preview": "vite preview",
        "start": "node server/integrated.js",
        "integrated": "node server/integrated.js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
        "debug-build": "ls -la && ls -la dist || echo 'No dist dir' && ls -la src || echo 'No src dir'",
        "heroku-postbuild": "npm run build"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.27.1",
        "@babel/preset-env": "^7.27.1",
        "@babel/preset-react": "^7.27.1",
        "@eslint/js": "^9.25.1",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.3",
        "@vitejs/plugin-react": "^4.3.4",
        "babel-jest": "^29.7.0",
        "eslint": "^9.21.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "msw": "^2.7.5",
        "vite": "^6.2.0"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@google/generative-ai": "^0.24.1",
        "@mui/icons-material": "^7.0.1",
        "@mui/material": "^7.0.1",
        "@mui/x-date-pickers": "^7.28.3",
        "axios": "^1.8.4",
        "chart.js": "^4.4.8",
        "cors": "^2.8.5",
        "date-fns": "^2.30.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "json2csv": "^6.0.0-alpha.2",
        "mongoose": "^8.0.0",
        "react": "^19.0.0",
        "react-chartjs-2": "^5.3.0",
        "react-dom": "^19.0.0",
        "react-router-dom": "^7.5.0",
        "redis": "^5.0.0"
    }
}

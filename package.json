{
    "name": "nutribyte",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "dev": "vite --config vite.config.js --host --port 8080",
        "build": "vite build --config vite.config.js",
        "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "start": "node server/integrated.js",
        "integrated": "node server/integrated.js",
        "zybooks": "vite build --config vite.config.js && SERVE_STATIC=true node server/integrated.js",
        "debug-build": "ls -la && ls -la dist || echo 'No dist dir' && ls -la src || echo 'No src dir'",
        "heroku-postbuild": "npm run build"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.21.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "vite": "^6.2.0"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^7.0.1",
        "@mui/material": "^7.0.1",
        "@mui/x-date-pickers": "^7.28.3",
        "axios": "^1.8.4",
        "chart.js": "^4.4.8",
        "date-fns": "^2.30.0",
        "react": "^19.0.0",
        "react-chartjs-2": "^5.3.0",
        "react-dom": "^19.0.0",
        "react-router-dom": "^7.5.0",
        "express": "^4.18.2",
        "mongoose": "^8.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1"
    }
}